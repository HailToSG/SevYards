<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<configuration xmlns="https://cm5.intertrust.ru/config">

    <!-- Cущность "Иное" и сущности, относящиеся к классу Иное -->

    <!-- Cущность "Иное" -->
    <domain-object-type name="other_others">
        <fields>
            <!--Ссылка на инвентаризацию -->
            <reference name="inventory_id" type="inventory"/>

            <!--Ссылка на id территории -->
            <reference name="territory_id" type="ter_territory"/>

            <!--тип объекта класса Иное -->
            <string name="type" length="1024">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </string>

            <!--состояние объекта класса Иное-->
            <string name="state" length="1024">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </string>
        </fields>

        <attachment-types>
            <attachment-type name="others_photo"/>
        </attachment-types>
    </domain-object-type>

    <!-- Cущность "Пандус" относится к классу Иное-->
    <domain-object-type name="other_rampant">
        <fields>

            <!-- Ссылка на инвентаризацию -->
            <reference name="inventory_id" type="inventory"/>

            <!--Ширина -->
            <decimal name="width">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>

            <!-- Перепад высот -->
            <decimal name="height_difference">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>

            <!-- Покрытие -->
            <decimal name="coating">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>
            
            <!-- Является ли откидным -->
            <boolean name="is_flap"/>

            <!-- Состояние объекта класса "Пандус" -->
            <decimal name="state">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>
        </fields>
        <attachment-types>
            <attachment-type name="other_rampant_photo"/>
        </attachment-types>
    </domain-object-type>

    <!-- Cущность "Устройство преграждения пути" относится к классу Иное -->
    <domain-object-type name="others_barrier">
        <fields>

            <!--Ширина проезда -->
            <long name="passage_width">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </long>

            <!-- Тип устройства -->
            <decimal name="kind_of_barrier">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>

            <!-- Материал-->
            <decimal name="material">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>

            <!-- Механизация-->
            <decimal name="mechanization">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>

            <!--Ссылка на инвентаризацию -->
            <reference name="inventory_id" type="inventory"/>

            <!--состояние объекта класса "Устройство для преграждения пути"-->
            <decimal name="state">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>
        </fields>
        <attachment-types>
            <attachment-type name="others_barrier_photo"/>
        </attachment-types>
    </domain-object-type>

    <!-- Cущность "Светильники" относится к классу "Иное"-->
    <domain-object-type name="other_lighting_device">
        <fields>

            <!--Ссылка на инвентаризацию -->
            <reference name="inventory_id" type="inventory"/>

            <!-- Тип-->
            <decimal name="kind_of_device">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>

            <!--Высота опоры -->
            <decimal name="height">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>

            <!-- Тип опоры-->
            <decimal name="type">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>

            <!-- Состояние покрытия-->
            <decimal name="coating_state">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>

            <!--состояние объекта класса "Светильники"-->
            <decimal name="state">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>
        </fields>
        <attachment-types>
            <attachment-type name="lighting_device_photo"/>
        </attachment-types>
    </domain-object-type>

    <!-- Cущность "Информационные стенды" относится к классу "Иное"-->
    <domain-object-type name="other_information_stand">

        <fields>
            <!-- Назначение-->
            <string name="purpose" length="1024">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </string>

            <!--Ссылка на инвентаризацию -->
            <reference name="inventory_id" type="inventory"/>

            <!--состояние объекта класса "Информационные стенды"-->
            <decimal name="state">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>
        </fields>
        <attachment-types>
            <attachment-type name="information_stand_photo"/>
        </attachment-types>
    </domain-object-type>

    <!-- Cущность "Ограждения" относится к классу "Иное"-->
    <domain-object-type name="other_fences">
        <fields>
            <!--Протяженность -->
            <long name="length">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </long>

            <!-- Материал-->
            <decimal name="material">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>

            <!-- Состояние покрытия-->
            <decimal name="coating_state">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>

            <!-- Тип ограждения -->
            <decimal name="fences_type">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>

            <!--Ссылка на инвентаризацию -->
            <reference name="inventory_id" type="inventory"/>

            <!--состояние объекта класса "Ограждения"-->
            <decimal name="state">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>
        </fields>
        <attachment-types>
            <attachment-type name="fences_photo"/>
        </attachment-types>
    </domain-object-type>

    <!-- Cущность "Водоемы" относится к классу "Иное"-->
    <domain-object-type name="other_pond">
        <fields>
            <!--Ссылка на инвентаризацию -->
            <reference name="inventory_id" type="inventory"/>

            <!-- Тип водоёма-->
            <decimal name="pond_type">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>

            <!--состояние объекта класса "Водоёмы"-->
            <decimal name="state">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>
        </fields>
        <attachment-types>
            <attachment-type name="pond_photo"/>
        </attachment-types>
    </domain-object-type>

    <!-- Cущность "Люки подземных коммуникаций" относится к классу "Иное"-->
    <domain-object-type name="other_underground_hatch">

        <fields>
            <!-- Тип люка -->
            <decimal name="kind_of_hatch">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>

            <!--Ссылка на инвентаризацию -->
            <reference name="inventory_id" type="inventory"/>

        </fields>
        <attachment-types>
            <attachment-type name="underground_hatch_photo"/>
        </attachment-types>
    </domain-object-type>

    <!-- Cущность "Опоры ЛЭП" относится к классу "Иное"-->
    <domain-object-type name="other_power_pylon">
        <fields>

            <!--Высота опоры -->
            <decimal name="height_pylon">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>

            <!-- Тип опоры-->
            <decimal name="type_pylon">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>

            <!--Ссылка на инвентаризацию -->
            <reference name="inventory_id" type="inventory"/>

            <!--состояние объекта класса "Опоры ЛЭП"-->
            <decimal name="state_pylon">
                <constraints>
                    <simple-constraint value="not-empty"/>
                </constraints>
            </decimal>
        </fields>
        <attachment-types>
            <attachment-type name="power_pylon_photo"/>
        </attachment-types>
    </domain-object-type>

</configuration>