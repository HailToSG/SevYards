<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<configuration xmlns="https://cm5.intertrust.ru/config">
    <navigation name="default-panel" is-default="true">

        <link name="Buildings" display-text="Строения" image="images/cases.png"
              child-to-open="Build_buildings">
            <child-links>

                <link name="Build_buildings" display-text="Строения">
                    <plugin>
                        <domain-object-surfer domain-object-type-to-create="build_buildings">
                            <collection-viewer>
                                <collection-ref name="buildings_collection"/>
                                <collection-view-ref name="buildings_collection_view"/>
                            </collection-viewer>
                            <form-viewer>
                                <form-mapping domain-object-type="build_buildings" form="buildings_form"/>
                            </form-viewer>
                        </domain-object-surfer>
                    </plugin>
                </link>

                <link name="Residential_buildings" display-text="Жилые строения">
                    <plugin>
                        <domain-object-surfer domain-object-type-to-create="build_residential">
                            <collection-viewer>
                                <collection-ref name="bld_residential_collection_extended"/>
                                <collection-view-ref name="bld_residential_collection_extended_view"/>
                            </collection-viewer>
                            <form-viewer>
                                <form-mapping domain-object-type="build_residential" form="bld_residential_form"/>
                            </form-viewer>
                        </domain-object-surfer>
                    </plugin>
                </link>

