<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<configuration xmlns="https://cm5.intertrust.ru/config" xmlns:act="https://cm5.intertrust.ru/config/action">
    <act:tool-bar useDefault="false"/>

    <navigation name="default-panel" is-default="true">
        <link name="Territories" display-text="Территории" image="images/docs" child-to-open="ter_territory">
            <child-links>
                <link name="ter_territory" display-text="Территории">
                    <plugin>
                        <domain-object-surfer domain-object-type-to-create="ter_territory">
                            <collection-viewer>
                                <act:tool-bar useDefault="false">
                                    <act:action-group name="create.visual.parameters.objects1" text="Создать территорию"
                                                      merged="true" imageClass="actionBar-create" order="30">
                                        <act:action-ref name-ref="aCreate" text="Общественная">
                                            <act:action-params>
                                                <act:action-param name="create.object.type" value="ter_territory"/>
                                                <act:action-param name="create.object.form"
                                                                  value="territories_public_form"/>
                                            </act:action-params>
                                        </act:action-ref>
                                        <act:action-ref name-ref="aCreate" text="Дворовая">
                                            <act:action-params>
                                                <act:action-param name="create.object.type" value="ter_territory"/>
                                                <act:action-param name="create.object.form"
                                                                  value="territories_yard_form"/>
                                            </act:action-params>
                                        </act:action-ref>
                                        <act:action-ref name-ref="aCreate" text="ИЖС">
                                            <act:action-params>
                                                <act:action-param name="create.object.type" value="ter_territory"/>
                                                <act:action-param name="create.object.form"
                                                                  value="territories_private_form"/>
                                            </act:action-params>
                                        </act:action-ref>
                                        <act:action-ref name-ref="aCreate" text="ЮЛ и ИП">
                                            <act:action-params>
                                                <act:action-param name="create.object.type" value="ter_territory"/>
                                                <act:action-param name="create.object.form"
                                                                  value="territories_ulip_form"/>
                                            </act:action-params>
                                        </act:action-ref>
                                    </act:action-group>
                                </act:tool-bar>

                                <collection-ref name="ter_collection"/>
                                <collection-view-ref name="ter_collection_view"/>
                            </collection-viewer>
                            <form-viewer>
                                <form-mapping domain-object-type="ter_territory" form="territories_form"/>
                            </form-viewer>
                        </domain-object-surfer>
                    </plugin>
                </link>
                <link name="ter_public_territories" display-text="Общественные территории">
                    <plugin>
                        <domain-object-surfer domain-object-type-to-create="ter_public">
                            <collection-viewer>
                                <collection-ref name="ter_public_collection"/>
                                <collection-view-ref name="ter_public_collection_view"/>
                            </collection-viewer>
                            <form-viewer>
                                <form-mapping domain-object-type="ter_public" form="ter_public_form"/>
                            </form-viewer>
                        </domain-object-surfer>
                    </plugin>
                </link>

                <link name="ter_yard_territories" display-text="Дворовые территории">
                    <plugin>
                        <domain-object-surfer domain-object-type-to-create="ter_yard">
                            <collection-viewer>
                                <collection-ref name="ter_yard_collection"/>
                                <collection-view-ref name="ter_yard_collection_view"/>
                            </collection-viewer>
                            <form-viewer>
                                <form-mapping domain-object-type="ter_yard" form="ter_yard_form"/>
                            </form-viewer>
                        </domain-object-surfer>
                    </plugin>
                </link>

                <link name="ter_private_territories" display-text="ИЖС - объекты">
                    <plugin>
                        <domain-object-surfer domain-object-type-to-create="ter_private_housing">
                            <collection-viewer>
                                <collection-ref name="ter_private_housing_collection"/>
                                <collection-view-ref name="ter_private_housing_collection_view"/>
                            </collection-viewer>
                            <form-viewer>
                                <form-mapping domain-object-type="ter_private_housing" form="ter_private_form"/>
                            </form-viewer>
                        </domain-object-surfer>
                    </plugin>
                </link>

                <link name="ter_ulip_territories" display-text="Объекты недвиж. имущ. и з.у. у ЮЛ и ИП">
                    <plugin>
                        <domain-object-surfer domain-object-type-to-create="ter_ulip">
                            <collection-viewer>
                                <collection-ref name="ter_ulip_collection"/>
                                <collection-view-ref name="ter_ulip_collection_view"/>
                            </collection-viewer>
                            <form-viewer>
                                <form-mapping domain-object-type="ter_ulip" form="ter_ulip_form"/>
                            </form-viewer>
                        </domain-object-surfer>
                    </plugin>
                </link>

            </child-links>
        </link>

        <link name="Inventory" display-text="Инвентаризация" image="images/docs" child-to-open="Invent">
            <child-links>

                <link name="Invent" display-text="Инвентаризации">
                    <plugin>
                        <domain-object-surfer domain-object-type-to-create="inventory">
                            <collection-viewer>
                                <collection-ref name="inventory_collection"/>
                                <collection-view-ref name="inventory_collection_view"/>
                            </collection-viewer>
                            <form-viewer>
                                <form-mapping domain-object-type="inventory" form="inventory_form"/>
                            </form-viewer>
                        </domain-object-surfer>
                    </plugin>
                </link>


            </child-links>
        </link>

    </navigation>
    <navigation-panel-mappings name="nav-panel-map1">
        <navigation-panel-mapping name="default-panel">
            <users>
                <user uid="admin"/>
            </users>
            <groups>
                <group-name value="test-group-1" priority="20"/>
            </groups>
        </navigation-panel-mapping>
    </navigation-panel-mappings>
</configuration>